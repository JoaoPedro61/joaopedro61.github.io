  "auto-changelog": {
    "commitLimit": false,
    "unreleased": true,
    "issueUrl": "PUT_YOUT_LINK_PREFIX_ISSUE_HERE/%7Bid%7D",
    "replaceText": {
      "[Ff]eature:": "",
      "[Ff]ix:": "",
      "[Bb]reak:": "",
      "([A-Z]+-d+)": "[$1](PUT_YOUT_LINK_PREFIX_ISSUE_HERE/$1) - "
